<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CRUDMAUIV3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CRUDMAUIV3.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="CRUDMAUIV3">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Details"
        ContentTemplate="{DataTemplate local:Details}"
        Route="Details" />

</Shell>
